{
    "compilerOptions": {
        "target": "ES2019",
        "lib": [
            "esnext"
        ],
        "experimentalDecorators": true,
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "module": "commonjs",
        "outDir": "build",
        "rootDir": "src",
        "moduleResolution": "node",
        "types": [
            "google-apps-script"
        ]
    },
    "include": [
        "src/**/*"
    ]
}